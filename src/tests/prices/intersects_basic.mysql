select `range_high`,`range_low` from `prices_adv` ORDER BY RAND() LIMIT 1 INTO @maxPrice,@minPrice;
--  ----------------------------------------------------------------------------
--  Test of the Intersects Combined Relation
-- -----------------------------------------------------------------------------
SELECT SQL_NO_CACHE  `b`.`price_id`,round(`b`.`range_low`/100,2),round(`b`.`range_high`/100,2) FROM ( SELECT `price_id` FROM `prices_basic` WHERE `range_low` <= @maxPrice AND `range_high` > @minPrice ORDER BY `range_high` ASC LIMIT 10) a, prices_basic b WHERE `a`.`price_id` = `b`.`price_id`;