select `range_high`,`range_low` from `prices_adv` ORDER BY RAND() LIMIT 1 INTO @maxPrice,@minPrice;
--  ----------------------------------------------------------------------------
--  Test of the Overlaps Allens relation
-- -----------------------------------------------------------------------------
SELECT SQL_NO_CACHE `b`.`price_id`,round(`b`.`range_low`/100,2),round(`b`.`range_high`/100,2) FROM ( SELECT `price_id` FROM `prices_basic` WHERE `range_low` < @range_low AND  `range_high` > @range_low  AND  `range_high` < @range_high) a, prices_basic b WHERE `a`.`price_id` = `b`.`price_id` LIMIT 10;